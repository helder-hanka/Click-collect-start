   <div className=\"row\">\n                  <div className=\"col-md-6 mb-3\">\n                    <label htmlFor=\"firstName\">First name</label>\n                    <input \n                      className=\"form-control\"\n                      type=\"text\" \n                      name=\"givenName\" \n                      id=\"firstName\" \n                      placeholder=\"please enter first name\"\n                      value={ clientDetails.givenName}\n                      onChange={handleOnChange}\n                      />\n                    <small style={ clientDetails.givenName ? styles.valid : styles.errors } >\n                      Valid first name is required error.\n                    </small>\n                  </div>\n\n                  <div className=\"col-md-6 mb-3\">\n                    <label htmlFor=\"lastName\">Last name</label>\n                    <input \n                      className=\"form-control\"\n                      type=\"text\" \n                      name=\"familyName\"  \n                      id=\"lastName\"  \n                      placeholder=\"please enter last name\"\n                      value={ clientDetails.familyName}\n                      onChange={handleOnChange}\n                    />\n                    <small style={ clientDetails.familyName ? styles.valid : styles.errors }>\n                      Valid last name is required.\n                    </small>\n                  </div>\n                </div>\n                \n                <div className=\"mb-3\">\n                  <label htmlFor=\"email\">Email <span className=\"text-muted\">(Optional)</span></label>\n                  <input \n                    className=\"form-control\"\n                    type=\"email\" \n                    name=\"email\"   \n                    id=\"email\" \n                    placeholder=\"you@example.com\" \n                    value={ clientDetails.email}\n                    onChange={handleOnChange}\n                  />\n                  <div style={ clientDetails.email ? styles.valid : styles.errors}>\n                    Please enter a valid email address for order updates\n                  </div>\n                </div>\n               \n                <button className=\"btn btn-primary btn-lg btn-block\" type=\"submit\" disabled={!isValid}>\n                    Continue to checkout \n                </button>\n              </form>\n            </div>\n          </div>\n        </div>\n      </section>\n  );\n}\nexport default Checkout;\n","/Users/helderfernandes/Formation_Udemy/click-collect-starter/src/components/Orders/index.js",[],"/Users/helderfernandes/Formation_Udemy/click-collect-starter/src/components/Layout/index.js",[],"/Users/helderfernandes/Formation_Udemy/click-collect-starter/src/components/Home/index.js",[],"/Users/helderfernandes/Formation_Udemy/click-collect-starter/src/components/Payment/Input.js",[],"/Users/helderfernandes/Formation_Udemy/click-collect-starter/src/components/Cart/CartFooter.js",[],"/Users/helderfernandes/Formation_Udemy/click-collect-starter/src/components/Layout/Header.js",[],"/Users/helderfernandes/Formation_Udemy/click-collect-starter/src/components/Layout/Footer.js",[],"/Users/helderfernandes/Formation_Udemy/click-collect-starter/src/components/components.js",[],"/Users/helderfernandes/Formation_Udemy/click-collect-starter/src/components/Home/Sidebar.js",[],"/Users/helderfernandes/Formation_Udemy/click-collect-starter/src/components/Home/Filters.js",[],"/Users/helderfernandes/Formation_Udemy/click-collect-starter/src/components/Home/Gallery.js",[],"/Users/helderfernandes/Formation_Udemy/click-collect-starter/src/lib/queries.js",[],"/Users/helderfernandes/Formation_Udemy/click-collect-starter/src/components/Home/Card.js",[],"/Users/helderfernandes/Formation_Udemy/click-collect-starter/src/context/index.js",["128"],"import React from \"react\";\nexport const Context = React.createContext();\n\nconst FiltersProvi